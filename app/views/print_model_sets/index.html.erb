<h1>Listing print_model_sets</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Default infill</th>
    <th>Default fill pattern</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @print_model_sets.each do |print_model_set| %>
  <tr>
    <td><%= print_model_set.name %></td>
    <td><%= print_model_set.default_infill %></td>
    <td><%= print_model_set.default_fill_pattern %></td>
    <td><%= link_to 'Show', print_model_set %></td>
    <td><%= link_to 'Print', new_print_job_batch_path(:print_job_batch => {:print_model_set_id => print_model_set.id}) %></td>
    <td><%= link_to 'Destroy', print_model_set, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Print model set', new_print_model_set_path %>
